[window]
dynamic_padding = true
padding.x = 1
padding.y = 0
opacity = 1
decorations = "none"

[scrolling]
history = 10000
multiplier = 3

[font]
size = 12
offset.x=0
offset.y=1

[font.normal]
family = "JetBrainsMono Nerd Font Mono"
style = "Medium"
[font.bold]
family = "JetBrainsMono Nerd Font Mono"
style = "ExtraBold"
[font.italic]
family = "JetBrainsMono Nerd Font Mono"
style = "Bold Italic"
[font.bold_italic]
family = "JetBrainsMono Nerd Font Mono"
style = "ExtraBold Italic"
